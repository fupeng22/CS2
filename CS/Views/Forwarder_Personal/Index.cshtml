@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>Index</title>
    <script src="@Url.Content("~/Scripts/jquery-1.4.4.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JqueryUI/jquery.easyui.min.js")" type="text/javascript"></script>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/black/easyui.css")" rel="stylesheet"
        title="black" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/bootstrap/easyui.css")" rel="stylesheet"
        title="bootstrap"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/cupertino/easyui.css")" rel="stylesheet"
        title="cupertino"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/dark-hive/easyui.css")" rel="stylesheet"
        title="dark-hive"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/default/easyui.css")" rel="stylesheet"
        title="default"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/gray/easyui.css")" rel="stylesheet"
        title="gray"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/metro/easyui.css")" rel="stylesheet"
        title="metro"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/metro-blue/easyui.css")" rel="stylesheet"
        title="metro-blue"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/metro-gray/easyui.css")" rel="stylesheet"
        title="metro-gray"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/metro-green/easyui.css")" rel="stylesheet"
        title="metro-green"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/metro-orange/easyui.css")" rel="stylesheet"
        title="metro-orange"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/metro-red/easyui.css")" rel="stylesheet"
        title="metro-red"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/pepper-grinder/easyui.css")" rel="stylesheet"
        title="pepper-grinder"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/sunny/easyui.css")" rel="stylesheet"
        title="sunny"/>
    <link href="@Url.Content("~/JqueryUI/themes/icon.css")"  rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/JS/Forwarder/Personal.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JqueryUI/easyui-lang-zh_CN.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JS/Common/Common.js")" type="text/javascript"></script>
</head>
<body>
    <div id="toolBar" style="padding: 0px; height: auto">
        <div>
            <form id="form_FileUpload" method="post" enctype="multipart/form-data">
            添加文件
            <input type="file" id="myFile" name="myFile" style="width: 400px;" />
            <input type="hidden" id="myFile_hid" name="myFile_hid" style="width: 400px;" />
            <span>温馨提示：最多可同时上传 1个文件，只允许上传 Excel 文件。<a href="#" id="btnUpLoad" name="btnUpLoad"
                class="easyui-linkbutton" data-options="iconCls:'icon-mini-upload'">开始上传</a>&nbsp;&nbsp;<a
                    href="#" id="btnSave" name="btnSave" class="easyui-linkbutton" data-options="iconCls:'icon-save'">保存数据</a>&nbsp;&nbsp;
            </span>
            </form>
        </div>
        <div class="easyui-panel" fit="true" title="总运单" style="height: 100px">
            <form id="MainWayBillForm" method="post">
            <table>
                <tr>
                    <td>
                        总运单号
                    </td>
                    <td>
                        <input id="txtSeriaNum" name="txtSeriaNum" readonly="true">
                    </td>
                    <td>
                        运输工具航次
                    </td>
                    <td>
                        <input id="txtWbVoyage" name="txtWbVoyage" readonly="true">
                    </td>
                    <td>
                        舱单进出口标志
                    </td>
                    <td>
                        <input id="txtWbIOmark" name="txtWbIOmark" readonly="true">
                    </td>
                    <td>
                        中文运输工具
                    </td>
                    <td>
                        <input id="txtWbChinese" name="txtWbChinese" readonly="true">
                    </td>
                    <td rowspan="3" style="padding: 3px; color: Red; font-weight: bold">
                        1、导入出错或需要修改时可重新导入<br />
                        2、重复导入相同总运单号时将会覆盖<br />
                        之前导入的数据,请慎重<br />
                    </td>
                </tr>
                <tr>
                    <td>
                        英文运输工具
                    </td>
                    <td>
                        <input id="txtWbEnglish" name="txtWbEnglish" readonly="true">
                    </td>
                    <td>
                        运输方式
                    </td>
                    <td>
                        <input id="txtWbTransportMode" name="txtWbTransportMode" readonly="true">
                    </td>
                    <td>
                        进港日期
                    </td>
                    <td>
                        <input id="txtWbEntryDate" name="txtWbEntryDate" readonly="true">
                    </td>
                    <td>
                        起运港/抵运地
                    </td>
                    <td>
                        <input id="txtWbSRport" name="txtWbSRport" readonly="true">
                    </td>
                </tr>
                <tr>
                    <td>
                        进出口岸代码
                    </td>
                    <td>
                        <input id="txtWbPortCode" name="txtWbPortCode" readonly="true">
                    </td>
                    <td>
                        总重量
                    </td>
                    <td>
                        <input id="txtWbTotalWeight" name="txtWbTotalWeight" readonly="true">
                    </td>
                    <td>
                        总件数
                    </td>
                    <td>
                        <input id="txtWbTotalNumber" name="txtWbTotalNumber" readonly="true">
                    </td>
                    <td>
                        分运单数
                    </td>
                    <td>
                        <input id="txtSubNumber" name="txtSubNumber" readonly="true">
                    </td>
                </tr>
            </table>
            </form>
        </div>
        <div class="easyui-panel" title="子运单" style="height: 1px">
        </div>
    </div>
    <div id="DG_SubWayBill" fit="true" style="width: 800px; height: 600px">
    </div>
</body>
</html>
