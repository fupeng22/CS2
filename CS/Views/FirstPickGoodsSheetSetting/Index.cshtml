<!DOCTYPE html>
<html>
<head>
    <title>Index</title>
    <script  src="@Url.Content("~/JS/Common/FirstPickGoodsSheetSetting.js")" type="text/javascript"></script>
</head>
<body>
    <div>
        <table>
            <tr>
                <td colspan="12" align="center">
                    <span style="font-weight: bold; font-size: large;">快件提货单打印设置信息</span><span id="lblTip"></span><hr />
                </td>
            </tr>
            <tr>
                <td align="right">
                    提货单流水号:
                </td>
                <td>
                    <input  type="text" id="FlowNum_ForPrint" name="FlowNum_ForPrint" value='@ViewData["FlowNum_ForSetting"]' />
                </td>
                <td align="right">
                    主运单号:
                </td>
                <td>
                    <input  type="text" id="wbSerialNum_ForPrint" name="wbSerialNum_ForPrint" value='@ViewData["wbSerialNum_ForSetting"]' readonly/>
                    <input  type="hidden" id="wbID_ForPrint" name="wbID_ForPrint" value='@ViewData["wbID_ForSetting"]'/>
                </td>
                <td align="right">
                    业务类型:
                </td>
                <td>
                    <input  type="text" id="CustomCategory_ForSetting" name="CustomCategory_ForSetting" value='@ViewData["CustomCategory_ForSetting"]' readonly/>
                    <input  type="hidden" id="hid_CustomCategory_ForSetting" name="hid_CustomCategory_ForSetting" value='@ViewData["hid_CustomCategory_ForSetting"]'/>
                </td>
                <td align="right">
                    总件数:
                </td>
                <td>
                    <input  type="text" id="wbSerialNumber_ForPrint" name="wbSerialNumber_ForPrint" value='@ViewData["wbSerialNumber_ForPrint"]' readonly style="width:100px"/>
                </td>
                <td align="right">
                    分运单件数:
                </td>
                <td>
                    <input  type="text" id="swbTotalNumber_ForPrint" name="swbTotalNumber_ForPrint" value='@ViewData["swbTotalNumber_ForPrint"]' readonly style="width:100px"/>
                </td>
                <td align="right">
                    重量:
                </td>
                <td>
                    <input  type="text" id="wbActualWeight_ForPrint" name="wbActualWeight_ForPrint" value='@ViewData["wbActualWeight_ForPrint"]' readonly style="width:100px"/>
                </td>
            </tr>
            <tr>
                <td align="right">
                    入库日期:
                </td>
                <td>
                    <input type="text" id="InStoreDate_ForSetting" name="InStoreDate_ForSetting" onfocus="WdatePicker();"
                class="Wdate" style="width: 90px"  value='@ViewData["InStoreDate_ForSetting"]'/>
                </td>
                <td align="right">
                    出库日期:
                </td>
                <td>
                    <input type="text" id="PickGoodsDate_ForSetting" name="PickGoodsDate_ForSetting" onfocus="WdatePicker();"
                class="Wdate" style="width: 90px"  value='@(DateTime.Now.ToString("yyyy-MM-dd"))'/>
                </td>
                <td align="right">
                    放行件数:
                </td>
                <td>
                    <input  type="text" id="ReleaseNum_ForSetting" name="ReleaseNum_ForSetting" value='@ViewData["ReleaseNum_ForSetting"]'  style="width:100px"/>
                </td>
                <td align="right">
                    扣留件数:
                </td>
                <td>
                    <input  type="text" id="UnReleaseNum_ForSetting" name="UnReleaseNum_ForSetting" value='@ViewData["UnReleaseNum_ForSetting"]' style="width:100px"/>
                </td>
                <td align="right">
                    日期:
                </td>
                <td colspan="3">
                    @DateTime.Now.ToString("yyyy-MM-dd")
                </td>
            </tr>
            <tr>
                <td align="center" colspan="12">
                    扣留货物明细:
                </td>
            </tr>
            <tr>
                <td colspan="12">
                    <div id="DG_SubWayBillResult" style="width: 1200px; height: 350px">
                    </div>
                </td>
            </tr>
            <tr>
                <td colspan="12">
                    <table>
                        <tr>
                            <td align="right">
                                直航\转关:
                            </td>
                            <td>
                                <input type="text" id="ddlReceiptMode_ForSetting" name="ddlReceiptMode_ForSetting" />
                            </td>
                            <td align="right">
                                操作费:
                            </td>
                            <td>
                                <input  type="text" style="width:100px" id="OperateFee_ForSetting" name="OperateFee_ForSetting" value='@ViewData["OperateFee_ForSetting"]' />
                            </td>
                            <td align="right">
                                提货费:
                            </td>
                            <td>
                                <input  type="text"  style="width:100px" id="PickGoodsFee_ForSetting" name="PickGoodsFee_ForSetting" value='@ViewData["PickGoodsFee_ForSetting"]' />
                            </td>
                            <td align="right">
                                仓储费:
                            </td>
                            <td>
                                <input type="text" style="width:100px"  id="KeepGoodsFee_ForSetting" name="KeepGoodsFee_ForSetting"
                         value='@ViewData["KeepGoodsFee_ForSetting"]' />
                            </td>
                            <td align="right">
                                移库费:
                            </td>
                            <td>
                                <input type="text" style="width:100px"  id="ShiftGoodsFee_ForSetting" name="ShiftGoodsFee_ForSetting"
                          value='@ViewData["ShiftGoodsFee_ForSetting"]'/>
                            </td>
                            <td align="right">
                                代收仓储费:
                            </td>
                            <td>
                                <input type="text" style="width: 100px" id="CollectionFee_ForSetting" name="CollectionFee_ForSetting"
                                    value='0.00' />
                            </td>
                        </tr>
                        <tr>
                            <td align="right">
                                结算方式:
                            </td>
                            <td>
                                <input type="text" id="ddlPayMode_ForSetting" name="ddlPayMode_ForSetting" />
                            </td>
                            <td align="right">
                                应付单价:
                            </td>
                            <td>
                                <input type="text"  style="width:100px"  id="ShouldPayUnit_ForSetting" name="ShouldPayUnit_ForSetting"
                          value='@ViewData["ShouldPayUnit_ForSetting"]'/>
                            </td>
                            <td align="right">
                                应付金额:
                            </td>
                            <td>
                                <input type="text"  style="width:100px"  id="shouldPay_ForSetting" name="shouldPay_ForSetting"
                          value='@ViewData["shouldPay_ForSetting"]'/>
                            </td>
                            <td align="right">
                                客户名:
                            </td>
                            <td>
                                <input type="text"  style="width:100px"  id="wbCompany_ForSetting" name="wbCompany_ForSetting"
                          value='@ViewData["wbCompany_ForSetting"]'/>
                            </td>
                            <td align="right">
                                应付单位:
                            </td>
                            <td>
                                <input type="text" id="Receipt_ForSetting" name="Receipt_ForSetting" />
                            </td>
                            <td align="right">
                                合计:
                            </td>
                            <td>
                                <input type="text"  style="width:100px"  id="TotalFee_ForSetting" name="TotalFee_ForSetting"
                          value='@ViewData["TotalFee_ForSetting"]'/>
                            </td>
                        </tr>
                    </table>
                </td>
            </tr>
            <tr>
                <td colspan="12" align="center">
                    <a href="#" id="btnPrePrint" name="btnPrePrint" class="easyui-linkbutton" data-options="iconCls:'icon-print'">
                        打印预览</a>|<a href="#" id="btnPreExcel" name="btnPreExcel" class="easyui-linkbutton"
                            data-options="iconCls:'icon-excel'">导出查看</a> &nbsp;&nbsp;&nbsp; <a href="#" id="btnConfirmPrint"
                                name="btnConfirmPrint" class="easyui-linkbutton" data-options="iconCls:'icon-print'"
                                style="color: Red">正式打印</a>|<a href="#" id="btnConfirmExcel" name="btnConfirmExcel"
                                    class="easyui-linkbutton" data-options="iconCls:'icon-excel'" style="color: Red">正式导出</a>
                </td>
            </tr>
        </table>
    </div>
</body>
</html>
