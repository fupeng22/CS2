@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>Index</title>
    <script src="@Url.Content("~/Scripts/jquery-1.4.4.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JqueryUI/jquery.easyui.min.js")" type="text/javascript"></script>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/black/easyui.css")" rel="stylesheet"
        title="black" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/bootstrap/easyui.css")" rel="stylesheet"
        title="bootstrap"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/cupertino/easyui.css")" rel="stylesheet"
        title="cupertino"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/dark-hive/easyui.css")" rel="stylesheet"
        title="dark-hive"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/default/easyui.css")" rel="stylesheet"
        title="default"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/gray/easyui.css")" rel="stylesheet"
        title="gray"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/metro/easyui.css")" rel="stylesheet"
        title="metro"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/metro-blue/easyui.css")" rel="stylesheet"
        title="metro-blue"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/metro-gray/easyui.css")" rel="stylesheet"
        title="metro-gray"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/metro-green/easyui.css")" rel="stylesheet"
        title="metro-green"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/metro-orange/easyui.css")" rel="stylesheet"
        title="metro-orange"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/metro-red/easyui.css")" rel="stylesheet"
        title="metro-red"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/pepper-grinder/easyui.css")" rel="stylesheet"
        title="pepper-grinder"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/sunny/easyui.css")" rel="stylesheet"
        title="sunny"/>
    <link href="@Url.Content("~/JqueryUI/themes/icon.css")"  rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/JS/Huayu/ExceptionStore.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JqueryUI/easyui-lang-zh_CN.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JS/Common/Common.js")" type="text/javascript"></script>
</head>
<body>
    <div id="toolBar" style="padding: 2px; height: auto">
        <input type="hidden" id="hidSessionUserName" name="hidSessionUserName" value="@Session["Global_Huayu_UserName"]" />
        <div style="margin-bottom: 5px">
            <a href="#" class="easyui-linkbutton" id="btnSeleAll" iconcls="icon-seleall" plain="true">
                全选</a>|<a href="#" class="easyui-linkbutton" id="btnInverseSele" iconcls="icon-inversesele"
                    plain="true">反选</a>|<a href="#" class="easyui-linkbutton" id="btnInStore" iconcls="icon-instore"
                        plain="true">入仓</a>|<a href="#" class="easyui-linkbutton" id="btnOutStore" iconcls="icon-outstore"
                            plain="true">出仓</a>|<a href="#" class="easyui-linkbutton" id="btnHandleExceptionForSele"
                                iconcls="icon-exception_handle" plain="true">处理异常记录</a>|<a href="javascript:void(0)"
                                    id="menuButtonReport" class="easyui-menubutton" data-options="menu:'#menuButtons',iconCls:'icon-report'">报表</a>
            <div id="menuButtons" style="width: 150px;">
                <div id="btnPrint" name="btnPrint" data-options="iconCls:'icon-print'">
                    打印(PDF)</div>
                <div id="btnExcel" name="btnExcel" data-options="iconCls:'icon-excel'">
                    导出(Excel)</div>
            </div>
            <span style="color: Red; font-weight: bold">未预入库导致的异常记录无法进行出入仓操作，请先将其进行预入库</span>
            @*<br /><a href="#" class="easyui-linkbutton" id="btnHandleExceptionForImport"
                    iconcls="icon-exception_handle" plain="true">处理异常记录(通过导入)</a>*@
        </div>
        <div>
            报关日期:<input type="text" id="txtBeginD" name="txtBeginD" onfocus="WdatePicker();"
                class="Wdate" style="width: 90px"  value='@(DateTime.Now.ToString("yyyy-MM-dd"))'/>---<input type="text" id="txtEndD" name="txtEndD"
                    onfocus="WdatePicker();" class="Wdate" style="width: 90px"  value='@(DateTime.Now.ToString("yyyy-MM-dd"))'/>&nbsp;&nbsp;
            异常时间:<input type="text" id="txtExceptionBeginD" name="txtExceptionBeginD" onfocus="WdatePicker();"
                class="Wdate" style="width: 90px"  value='@(DateTime.Now.ToString("yyyy-MM-dd"))'/>---<input type="text" id="txtExceptionEndD"
                    name="txtExceptionEndD" onfocus="WdatePicker();" class="Wdate" style="width: 90px"  value='@(DateTime.Now.ToString("yyyy-MM-dd"))'/>&nbsp;&nbsp;报关货代公司:<input
                        type="text" id="txtVoyage" name="txtVoyage" />&nbsp;&nbsp; 异常类型:
            <select id="ddlExceptionType" class="easyui-combotree" multiple style="width: 270px;" panelHeight="auto">
            </select><br />
            异常状态:
            <select id="txtExceptionStatus" name="txtExceptionStatus" class="easyui-combobox"
                editable="false" panelheight="auto">
                <option value="---请选择---">---请选择---</option>
                <option value="0">待处理</option>
                <option value="1">正在处理</option>
                <option value="2">已处理</option>
            </select>
            &nbsp;&nbsp;总运单号<input type="text" id="txtCode" name="txtCode" />&nbsp;&nbsp;&nbsp;
            分运单号:<input type="text" id="txtSubWayBillCode" name="txtSubWayBillCode" />&nbsp;&nbsp;&nbsp;
            <a href="#" id="btnQuery" name="btnQuery" class="easyui-linkbutton" data-options="iconCls:'icon-search'">
                查询</a> <a href="#" id="btnReset" name="btnReset" class="easyui-linkbutton" data-options="iconCls:'icon-undo'">
                    重置</a>
        </div>
    </div>
    <div id="DG_WayBillResult" fit="true" style="width: 800px; height: 600px">
    </div>
    <div id="dlg_ExceptionHandler" data-options="iconCls:'icon-save'" style="padding: 5px;
        width: 400px; height: 300px;">
    </div>
</body>
</html>
