@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>Index</title>
    <script src="@Url.Content("~/Scripts/jquery-1.4.4.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JqueryUI/jquery.easyui.min.js")" type="text/javascript"></script>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/black/easyui.css")" rel="stylesheet"
        title="black" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/bootstrap/easyui.css")" rel="stylesheet"
        title="bootstrap"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/cupertino/easyui.css")" rel="stylesheet"
        title="cupertino"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/dark-hive/easyui.css")" rel="stylesheet"
        title="dark-hive"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/default/easyui.css")" rel="stylesheet"
        title="default"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/gray/easyui.css")" rel="stylesheet"
        title="gray"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/metro/easyui.css")" rel="stylesheet"
        title="metro"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/metro-blue/easyui.css")" rel="stylesheet"
        title="metro-blue"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/metro-gray/easyui.css")" rel="stylesheet"
        title="metro-gray"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/metro-green/easyui.css")" rel="stylesheet"
        title="metro-green"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/metro-orange/easyui.css")" rel="stylesheet"
        title="metro-orange"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/metro-red/easyui.css")" rel="stylesheet"
        title="metro-red"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/pepper-grinder/easyui.css")" rel="stylesheet"
        title="pepper-grinder"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/sunny/easyui.css")" rel="stylesheet"
        title="sunny"/>
    <link href="@Url.Content("~/JqueryUI/themes/icon.css")"  rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/JS/Huayu/Huayu_WayBillDailyReport.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JqueryUI/easyui-lang-zh_CN.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JS/Common/Common.js")" type="text/javascript"></script>
</head>
<body>
    <div id="toolBar" style="padding: 2px; height: auto">
        出库(提货)日期:<input type="text" id="txtBeginD" name="txtBeginD" onfocus="WdatePicker();"
                class="Wdate" style="width: 90px"  value='@(DateTime.Now.ToString("yyyy-MM-dd"))'/>---<input type="text" id="txtEndD" name="txtEndD"
                    onfocus="WdatePicker();" class="Wdate" style="width: 90px"  value='@(DateTime.Now.ToString("yyyy-MM-dd"))'/>&nbsp;
        总运单号:<input type="text" id="txtCode" name="txtCode" />&nbsp;放行单号:<input type="text"
            id="txtWbrCode" name="txtWbrCode" />&nbsp;&nbsp;&nbsp; <a href="#" id="btnQuery"
                name="btnQuery" class="easyui-linkbutton" data-options="iconCls:'icon-search'">查询</a>
        <a href="#" id="btnReset" name="btnReset" class="easyui-linkbutton" data-options="iconCls:'icon-undo'">
            重置</a>&nbsp;<a href="javascript:void(0)" id="menuButtonReport" class="easyui-menubutton"
                data-options="menu:'#menuButtons',iconCls:'icon-report',plain:false">报表</a>
        <div id="menuButtons" style="width: 150px;">
            <div id="btnPrint" name="btnPrint" data-options="iconCls:'icon-print'">
                打印(PDF)</div>
            <div id="btnExcel" name="btnExcel" data-options="iconCls:'icon-excel'">
                导出(Excel)</div>
        </div>
        <br />
        邮件接收人:<select id="sele_EmailReciever" name="sele_EmailReciever" class="easyui-combobox"
            editable="true" panelheight="auto" style="width: 250px">
            <option value="976675036@qq.com">976675036@qq.com</option>
        </select>&nbsp;&nbsp;<a href="javascript:void(0)" id="menuSendEmail" class="easyui-menubutton"
            data-options="menu:'#menuButtons_Email',iconCls:'icon-email',plain:false"> 发送邮件</a>
        <div id="menuButtons_Email" style="width: 150px;">
            <div id="btnEmail_PDF" name="btnEmail_PDF" data-options="iconCls:'icon-attachment'">
                附件以PDF格式&nbsp;<img  src="../../JqueryUI/themes/icons/pdf.png"/></div>
            <div id="btnEmail_Excel" name="btnEmail_Excel" data-options="iconCls:'icon-attachment'">
                附件以Excel格式&nbsp;<img   src="../../JqueryUI/themes/icons/Excel.png"/></div>
        </div>
        <font style="color: Red; font-weight: bold">邮件模板请点击【系统管理】-【邮件信息维护】中去修改</font>
    </div>
    <div id="DG_WayBillDailyReportResult" fit="true" style="width: 800px; height: 600px">
    </div>
</body>
</html>
