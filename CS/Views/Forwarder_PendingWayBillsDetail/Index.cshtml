<!DOCTYPE html>
<html>
<head>
    <title>Index</title>
    <script src="@Url.Content("~/JS/Forwarder/PendingWayBillsDetail.js")" type="text/javascript"></script>
</head>
<body>
    <div id="toolBarDetail" style="padding: 2px; height: auto">
        <input  type="hidden" id="hid_wbID" name="hid_wbID" value='@ViewData["Detail_wbID"]'/>
        <span>总运单号:<font style="font-weight: bold; color: Red">@ViewData["Detail_wbSerialNum"]</font>&nbsp;&nbsp;&nbsp;分运单号:<input
            type="text" id="txtSubWayBillCode" name="txtSubWayBillCode" /></span>&nbsp;<a href="#" id="btnQuery_Detail" name="btnQuery_Detail" class="easyui-linkbutton" data-options="iconCls:'icon-search'">
                查询</a>
    </div>
    <div id="DG_SubWayBillDetail" fit="true">
    </div>
    <div id="dlg_UpdateSwbNeedCheck" data-options="iconCls:'icon-edit'" style="padding: 5px;
        width: 200px; height: 150px;">
        <input type="hidden" id="hid_swbID" name="hid_swbID" />
        <table>
            <tr>
                <td>
                    分运单号：
                </td>
                <td>
                    <span id="span_swbSerialNum_First"></span>
                </td>
            </tr>
        </table>
    </div>
    <div id="dlg_UpdateCheckResult" data-options="iconCls:'icon-edit'" style="padding: 5px;
        width: 200px; height: 150px;">
        <input type="hidden" id="hid_swbdID" name="hid_swbdID" />
        <input type="hidden" id="hid_IDForUpdate" name="hid_IDForUpdate" />
        <table>
            <tr>
                <td>
                    分运单号：
                </td>
                <td>
                    <span id="span_swbSerialNum"></span>
                </td>
                <td>
                    品名：
                </td>
                <td>
                    <span id="span_swbDescription_CHN"></span>
                </td>
            </tr>
            <tr>
                <td>
                    查验结果：
                </td>
                <td colspan="3">
                    <input type="text" id="ddlCheckResult" name="ddlCheckResult" style="width: 100px" />
                    <input type="text" id="txtCheckResultDescription" name="txtCheckResultDescription"
                        style="width: 240px" />
                </td>
            </tr>
            <tr>
                <td>
                    处理意见：
                </td>
                <td colspan="3">
                    <input type="text" id="ddlHandleSuggestion" name="ddlHandleSuggestion" style="width: 100px" />
                    <input type="text" id="txtHandleSuggestionDescription" name="txtHandleSuggestionDescription"
                        style="width: 240px" />
                </td>
            </tr>
        </table>
    </div>
</body>
</html>
